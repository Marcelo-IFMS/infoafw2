            <h2>Usuário Autorizado</h2>
            <%}else{%>
                <h2>Usuário não Autorizado</h2>
                <%}%>
                    <% const frutas=[ 'banana' ,'manga','tomate','uva', 'laranja']%>
                        <table>
                            <tr>
                                <td>Lista de Frutas</td>
                            </tr>
                            <% for(i=0;i<frutas.length;i++){%>
                                <tr>
                                    <td>
                                        <%=frutas[i]%>
                                    </td>
                                </tr>
                                <%}%>
                        </table>

</body>

</html>